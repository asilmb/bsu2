{
    "name": "woop/yii2-wooppay-com",
    "description": "Wooppay web application",
    "keywords": ["yii2", "framework", "wooppay", "wallet", "payment"],
    "homepage": "http://wooppay.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
		"yiisoft/yii2": "2.0.*",
		
		"yiisoft/yii2-bootstrap": "2.0.6.0",
        "yiisoft/yii2-swiftmailer": "2.0.7",
		
		"bower-asset/admin-lte": "~2.0",
        "bower-asset/font-awesome": "~4.0",
		
		"kartik-v/yii2-widgets": "3.4.0.0",
		"comodojo/zip": "*",
		"mdmsoft/yii2-admin": "~2.0",
		"deesoft/yii2-console": "1.1.0.0",
        "zhuravljov/yii2-logreader": "*",
		
		"yii2lab/yii2-app": "dev-master",
		"yii2lab/yii2-init": "dev-master",
		"yii2lab/yii2-console": "dev-master",
		"yii2lab/yii2-migration": "dev-master",
		"yii2lab/yii2-rbac": "dev-master",
		"yii2lab/yii2-store": "dev-master",
		"yii2lab/yii2-test": "dev-master",
		"yii2lab/yii2-rest": "dev-master",
        "yii2lab/yii2-helpers": "dev-master",
        "yii2lab/yii2-misc": "dev-master",
		"yii2lab/yii2-user": "dev-master",
		"yii2lab/yii2-ubuntu-font": "dev-master",
		"yii2lab/yii2-validator": "dev-master",
		
		"yii2module/yii2-offline": "dev-master",
		"yii2module/yii2-environments": "dev-master",
		"yii2module/yii2-fixture": "dev-master",
		"yii2module/yii2-test": "dev-master",
		"yii2module/yii2-cleaner": "dev-master",
		"yii2module/yii2-error": "dev-master",
		"yii2module/yii2-rbac": "dev-master",
		"yii2module/yii2-lang": "dev-master",
		"yii2module/yii2-rest-client": "dev-master",
		"yii2module/yii2-restapidoc": "dev-master",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "yii2woop/yii2-tps": "dev-master",
		
		"php": ">=5.4.0",
        "paulzi/yii2-json-behavior": "^1.0",
        "yiisoft/yii2-imagine": "^2.1",
        "2amigos/yii2-qrcode-helper": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
		"yiisoft/yii2-apidoc": "~2.0.0",
		
		"guzzlehttp/guzzle": "*",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
